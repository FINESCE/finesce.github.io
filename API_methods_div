  <!-- Name -->
      <h1 class="resourceGroupName">
      Authentication    </h1>
    <!-- /Name -->

  <!-- Description -->
      <div class="resourceGroupDescription markdown formalTheme">
      <p>This set of APIs is used for authenticating users against oauth2 authentication and authorization services.</p>    </div>
    <!-- /Description -->

  <!-- Resources -->
  <div class="resources"><div class="resource2">

